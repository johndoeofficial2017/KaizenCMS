@{
    Layout = null;
}
<div class="jarviswidget" data-widget-colorbutton="false"
     data-widget-editbutton="false"
     data-widget-togglebutton="false"
     data-widget-deletebutton="false"
     data-widget-fullscreenbutton="false"
     data-widget-custombutton="false"
     data-widget-collapsed="false"
     data-widget-sortable="false">

    <header class="button-header">
        <a class="btn btn-default" ng-click="SaveData();"
           ng-show="CM00220.Status == 1">
            <i class="glyphicon glyphicon-floppy-disk icon-large">
            </i><br />
            Save &amp; Close
        </a>
        <a class="btn btn-default" ng-click="UpdateData();"
           ng-show="CM00220.Status == 2">
            <i class="glyphicon glyphicon-floppy-disk icon-large">
            </i><br />
            Update &amp; Close
        </a>
        <a class="btn btn-default" ng-click="Delete();" ng-show="CM00220.Status == 2"><i class="glyphicon glyphicon-trash"></i><br />Delete </a>
        <a class="btn btn-default" ng-click="Clear();"><i class="glyphicon glyphicon-remove"></i><br />Clear </a>
        <a class="btn btn-default"><i class="glyphicon glyphicon-print"></i><br />Print </a>
        <a class="btn btn-default" ng-click="Cancel();"><i class="glyphicon glyphicon-remove-circle"></i><br />Cancel </a>
    </header>
    <div class="no-border overflow">
        <div class="widget-body">
            <div class="row">
                <article class="col-md-12 col-lg-12">
                    <div class="row no-padding">
                        <form class="form-horizontal">
                            <fieldset>
                                <div class="row">
                                    <section class="col-md-12">
                                        <label class="col col-md-2 control-label">Transaction ID</label>
                                        <label class="input col col-md-4">
                                            <input type="text" class="form-control" placeholder="Transaction ID" maxlength="10"
                                                   ng-model="CM00220.TrxID" ng-disabled="CM00220.Status == 2" />
                                        </label>
                                    </section>
                                </div>
                                <div class="row">
                                    <section class="col-md-12">
                                        <label class="col-md-2 control-label">Agent</label>
                                        <label class="col-md-4">
                                            <div class="input-group">
                                                <input type="text" class="form-control" placeholder="Agent ID"
                                                       ng-model="CM00220.AgentID" readonly />
                                                <span class="input-group-btn input-group-sm">
                                                    <button class="btn btn-default"
                                                            ng-click="OpenkendoWindowForm('CMS','CMS_Agent','PopUp',null,'MainAgent')">
                                                        <span class="glyphicon glyphicon-search"></span>
                                                    </button>
                                                </span>
                                            </div>
                                        </label>
                                        <label class="col-md-2 control-label">Replaced Agent</label>
                                        <label class="col-md-4">
                                            <div class="input-group">
                                                <input type="text" class="form-control" placeholder="Replaced Agent ID"
                                                       ng-model="CM00220.AgentIDTO" readonly />
                                                <span class="input-group-btn input-group-sm">
                                                    <button class="btn btn-default"
                                                            ng-click="OpenkendoWindowForm('CMS','CMS_Agent','PopUp',null,'ReplacedAgent')">
                                                        <span class="glyphicon glyphicon-search"></span>
                                                    </button>
                                                </span>
                                            </div>
                                        </label>
                                    </section>
                                </div>
                                <div class="row">
                                    <section class="col-md-12">
                                        <label class="col-md-2 control-label">Start Date</label>
                                        <label class="col-md-4">
                                            <input kendo-date-picker k-format="'dd/MM/yyyy'"
                                                   k-ng-model="CM00220.StartDate" style="width: 100%;" />
                                        </label>
                                        <label class="col-md-2 control-label">End Date</label>
                                        <label class="col-md-4">
                                            <input kendo-date-picker k-format="'dd/MM/yyyy'"
                                                   k-ng-model="CM00220.EndDate" style="width: 100%;" />
                                        </label>
                                    </section>
                                </div>
                                <div class="row">
                                    <section class="col-md-12">
                                        <label class="col-md-2 control-label">Comments</label>
                                        <label class="col-md-4">
                                            <textarea class="form-control" ng-model="CM00220.TrxComments"
                                                      rows="4" placeholder="Comments"></textarea>
                                        </label>
                                    </section>
                                </div>
                            </fieldset>
                        </form>
                    </div>
                </article>
            </div>
        </div>
    </div>
</div>