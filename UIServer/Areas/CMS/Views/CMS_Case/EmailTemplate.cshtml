                                            <td class="tabletd text-primary"><strong>@Model.CaseAccountNo</strong></td>
                                        </tr>
                                        <tr>
                                            <td class="tabletd">Agent</td>
                                            <td class="tabletd text-primary"><strong>@Model.AgentID</strong></td>
                                        </tr>
                                        <tr>
                                            <td class="tabletd">Assign Date</td>
                                            <td class="tabletd text-primary"><strong>@Model.AssignDate.ToString("dd/MM/yyyy")</strong></td>
                                        </tr>
                                        @if (Model.TransactionDate != null)
                                        {
                                            <tr>
                                                <td class="tabletd">Transaction Date</td>
                                                <td class="tabletd text-primary"><strong>@Model.TransactionDate.Value.ToString("dd/MM/yyyy")</strong></td>
                                            </tr>
                                        }
                                        @if (Model.BookingDate != null)
                                        {
                                            <tr>
                                                <td class="tabletd">Booking Date</td>
                                                <td class="tabletd text-primary"><strong>@Model.BookingDate.Value.ToString("dd/MM/yyyy")</strong></td>
                                            </tr>
                                        }
                                        @if (Model.ClosingDate != null)
                                        {
                                            <tr>
                                                <td class="tabletd">Closing Date</td>
                                                <td class="tabletd text-primary"><strong>@Model.ClosingDate.Value.ToString("dd/MM/yyyy")</strong></td>
                                            </tr>
                                        }
                                        <tr>
                                            <td class="tabletd">Claim Amount</td>
                                            <td class="tabletd text-primary"><strong>@Model.ClaimAmount</strong></td>
                                        </tr>
                                        <tr>
                                            <td class="tabletd">Agent</td>
                                            <td class="tabletd text-primary"><strong>@Model.AgentID</strong></td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-12 col-md-5" style="float:right;display: table-cell;">
